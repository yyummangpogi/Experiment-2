#include<iostream>
#include<conio.h>
using namespace std;

int main()
{
	int counter;
	int next;
	
	cout<<"Counting...."<<endl;
	
	for(counter=1; counter<=10; counter++)
	{
		cout<<counter<<", ";

		if(counter==10)
		{
			for (next=12; next<=30; next+=2)
			{
		
				cout<<next<<", ";	
				
			}	
		}
	}
	
	
	
	
	_getch();
	return 0;
}


